---
# Ansible Inventory - Auto-generated from Terraform
# This is an EXAMPLE - actual file is generated by scripts/generate-ansible-inventory.sh

all:
  children:
    webservers:
      hosts:
        web1:
          ansible_host: <EC2_PUBLIC_IP>
        web2:
          ansible_host: <EC2_PUBLIC_IP>

      vars:
        ansible_user: ubuntu
        ansible_ssh_private_key_file: ~/.ssh/id_rsa
        ansible_ssh_common_args: '-o StrictHostKeyChecking=no'
        aws_region: eu-west-2
        db_host: <RDS_ENDPOINT>
        db_port: 5432
        db_name: appdb
        db_username: dbadmin
        db_secret_name: dev-db-password
        backend_image: "<ECR_URL>/dev-backend:latest"
        frontend_image: "<ECR_URL>/dev-frontend:latest"
        backend_port: 3000
        frontend_port: 80
        docker_build_enabled: false